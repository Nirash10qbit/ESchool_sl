<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".analyselesst"
    android:orientation="vertical"
    android:background="@drawable/gbg"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:orientation="vertical"
        android:background="@color/blue">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="4">
                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/ic_arrow_back_black_24dp"
                        android:onClick="backanalyseact"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4" >

                <TextView
                    android:id="@+id/subm"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:fontFamily="serif"
                    android:textColor="@color/white"
                    android:textAlignment="textEnd"
                    android:textSize="50dp"
                    android:textStyle="bold|italic"
                    tools:ignore="RtlCompat" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="%"
                    android:textColor="@color/white"
                    android:textSize="50dp"
                    android:textStyle="bold|italic" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tva2"
                    android:layout_width="0dp"
                    android:layout_height="0dp" />
                <TextView
                    android:id="@+id/tvlen"
                    android:layout_width="0dp"
                    android:text="0"
                    android:layout_height="0dp" />


            </LinearLayout>


        </LinearLayout>





    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.8"
        android:background="@color/blue">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAlignment="center"
            android:fontFamily="serif"
            android:textColor="@color/white"
            android:text="மொத்த சதவீதம்"
            android:textSize="13dp"
            android:textStyle="bold|italic" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.3"
        android:background="@color/blue">

        <TextView
            android:id="@+id/tvsu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAlignment="center"
            android:fontFamily="serif"
            android:textColor="@color/white"
            android:textSize="22dp"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.8"
        android:background="@color/blue">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textAlignment="center"
            android:fontFamily="serif"
            android:textColor="@color/white"
            android:text="உருவகப்படுத்துதல்களுக்கான"
            android:textSize="13dp"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.3"
        android:orientation="horizontal">


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0"
        android:orientation="horizontal">


        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="6"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:gravity="center_horizontal"
            android:textSize="13dp"
            android:textStyle="bold|italic"
            android:text="Select the given below bars in the bar chart"
            tools:ignore="RtlCompat" />




    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0"
        android:orientation="horizontal">


        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="6"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:gravity="center_horizontal"
            android:textSize="13dp"
            android:textStyle="bold|italic"
            android:text="for analyze"
            tools:ignore="RtlCompat" />




    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.8"
        android:orientation="vertical">
        <TextView
            android:id="@+id/se8"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/black"
            android:text="     மாணவர்களின் எண்ணிக்கை"
            android:textSize="9dp"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="vertical">

        <com.github.mikephil.charting.charts.BarChart
            android:id="@+id/barChart"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="10"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvasu"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            android:textAlignment="textEnd"
            android:text="science"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/se1"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:gravity="center_horizontal"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/se2"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="23"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            android:text="பதிவுசெய்யப்பட்ட மாணவர்களின்"
            tools:ignore="RtlCompat" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">




        <TextView
            android:id="@+id/se4"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            android:textAlignment="textEnd"
            android:text="மொத்த எண்ணிக்கை"
            tools:ignore="RtlCompat" />




        <TextView
            android:id="@+id/se3"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:gravity="center_horizontal"
            android:textSize="12dp"
            android:text=":"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/l6"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvsu2"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="12"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            android:textAlignment="textEnd"
            android:text="science"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/se5"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.3"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:gravity="center_horizontal"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/se6"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="10"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:textStyle="bold|italic"
            android:text="பதிவுசெய்யப்பட்ட "
            tools:ignore="RtlCompat" />



    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">




        <TextView
            android:id="@+id/se7"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:layout_weight="20"
            android:fontFamily="serif"
            android:textAlignment="textEnd"
            android:text="மாணவர்களின் மொத்த எண்ணிக்கை : "
            android:textSize="12dp"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

        <TextView
            android:id="@+id/l62"
            android:visibility="gone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:textSize="12dp"
            android:text="0"
            android:textStyle="bold|italic"
            tools:ignore="RtlCompat" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tvbsu"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:layout_gravity="center_horizontal"
            android:fontFamily="serif"
            android:textSize="13dp"
            android:gravity="center_horizontal"
            android:textStyle="bold|italic"
            android:text="science"
            tools:ignore="RtlCompat" />





    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5.5"
        android:orientation="vertical"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0"
        android:orientation="vertical">



        <TextView
            android:id="@+id/l1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <TextView
            android:id="@+id/l2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <TextView
            android:id="@+id/l3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <TextView
            android:id="@+id/l4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
        <TextView
            android:id="@+id/l5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />


    </LinearLayout>

</LinearLayout>